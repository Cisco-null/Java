import java.util.Scanner;
import java.util.Random;

public class olamundo {
     public static void main(String[] args) {

     
          var gerador = new Random();

          int num;
          int falha_critica;
          
          falha_critica = gerador.nextInt(20); //gera num aleatório até 20
          
          Scanner sc = new Scanner(System.in);
          num = sc.nextInt();
          num += falha_critica; // está somando o numero lido e o numero random
          
          System.out.printf("%d\n%d", falha_critica,num);

          if (1 == falha_critica) {
               System.out.println("Morreu");
          }
          else {
               System.out.println("Ta vivo"); 
          }
          
     
         
     }
    
}
