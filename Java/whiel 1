import java.util.Scanner;


public class Main{
     public static void main(String[] args) {
        
        var sc = new Scanner(System.in);


        int senha = sc.nextInt();

        while(senha !=2002){
            System.out.println("Senha Incorreta");
            senha = sc.nextInt();
        }
        
        System.out.println("Senha Correta");

        sc.close();
    }
}
